@model CreateContact


<input class="form-control mb-2" asp-for="Name" type="text" placeholder="Андрей.."/>
<input class="form-control mb-2" asp-for="MobilePhone" type="tel" placeholder="+375 (12) 345 67 89.."/>
<input class="form-control mb-2" asp-for="JobTitle" type="text" placeholder="Программист.."/>
<input class="form-control mb-2" asp-for="BirthDate" type="date" placeholder="11.09.2001.."/>

